{
  "name": "GrapeVine",
  "semanticHighlighting": true,
  "semanticTokenColors": {
      "magicFunction": "#68d7df",
      "selfParameter": "#a33cf7",
      "parameter": {
        "fontStyle": "italic",
        "foreground": "#de8cff"
      },
      "function.builtin": "#ff93f6",
      "class.builtin": "#8beeff",
      "module": "#c25ecb"
  },
  "tokenColors": [
    {
      "name": "[VSCODE-CUSTOM] JSON Property Name",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#ff52a3"
      }
    },
    {
      "name": "[VSCODE-CUSTOM] JSON Punctuation for Property Name",
      "scope": "support.type.property-name.json punctuation",
      "settings": {
        "foreground": "#ff52a3"
      }
    },
    {
      "scope": "source.python meta.class.python entity.name.type.class.python",
      "settings": {
        "foreground": "#fff01b",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#9d60ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.class.inheritance.python",
      "settings": {
        "foreground": "#d4c922",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "source",
      "settings": {
        "foreground": "#CD91FF"
      }
    },
    {
      "scope": "keyword.control.flow",
      "settings": {
        "foreground": "#9658DF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#7a7a7a"
      }
    },
    {
      "scope": "keyword.control.import",
      "settings": {
        "foreground": "#c560ff",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "meta.function-call",
      "settings": {
        "foreground": "#b397ff"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.numeric.bin",
        "constant.numeric.hex"
      ],
      "settings": {
        "foreground": "#f19946"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#e75ea2",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.function.parameters",
      "settings": {
        "foreground": "#de8cff",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "punctuation.separator",
      "settings": {
        "foreground": "#ffb8dc"
      }
    },
    {
      "scope": [
        "meta.attribute.python",
        "meta.member.access.python"
      ],
      "settings": {
        "foreground": "#EC3FE9"      
      }
    },
    {
      "scope": "support.type.exception",
      "settings": {
        "foreground": "#FFA5DB",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "constant.language.python",
        "constant.other"
      ],
      "settings": {
        "foreground": "#FF9D63"
      }
    },
    {
      "scope": "entity.name.namespace",
      "settings": {
        "foreground": "#C25ECB"
      }
    },
    {
      "scope": [
        "variable.other.readwrite",
        "variable",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#eeeeee"      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#96df66"
      }
    }
  ],
  "colors": {
    "activityBar.background": "#333842",
    "activityBar.foreground": "#D7DAE0",
    "editorInlayHint.background": "#2C313A",
    "editorInlayHint.foreground": "#636e83",
    "notebook.cellEditorBackground": "#2C313A",
    "activityBarBadge.background": "#528BFF",
    "activityBarBadge.foreground": "#D7DAE0",
    "button.background": "#4D78CC",
    "button.foreground": "#FFFFFF",
